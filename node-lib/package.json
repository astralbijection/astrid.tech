{
  "name": "@astrid.tech/node-lib",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "AGPLv3",
  "description": "Shared library utilities for node projects",
  "repository": "https://github.com/astralbijection/astrid.tech",
  "directory": "node-lib",
  "author": "Astrid Yu",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "npx ts-mocha -p tsconfig.json **/*.test.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@tsconfig/recommended": "^1.0.1",
    "@types/better-sqlite3": "^7.4.0",
    "@types/hsl-to-hex": "^1.0.0",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^16.9.1",
    "@types/seedrandom": "^3.0.1",
    "jest": "^27.2.0",
    "mocha": "^9.0.1",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "axios-jwt": "^1.7.1",
    "better-sqlite3": "^7.4.3",
    "hsl-to-hex": "^1.0.0",
    "rehype-dom-stringify": "^2.0.1",
    "rehype-highlight": "^5.0.0",
    "rehype-katex": "^5.0.0",
    "rehype-picture": "^3.0.0",
    "rehype-raw": "^5.1.0",
    "rehype-react": "^6.2.1",
    "rehype-stringify": "^8.0.0",
    "rehype-urls": "^1.1.1",
    "remark": "^13.0.0",
    "remark-emoji": "^2.2.0",
    "remark-excerpt": "^1.0.0-beta.1",
    "remark-footnotes": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-graphviz": "^0.2.2",
    "remark-grid-tables": "^2.1.2",
    "remark-html": "^13.0.2",
    "remark-math": "^4.0.0",
    "remark-oembed": "^1.2.2",
    "remark-parse": "^9.0.0",
    "remark-plain-text": "^0.2.0",
    "remark-rehype": "^8.1.0",
    "remark-slug": "^6.0.0",
    "remark-unwrap-images": "^2.1.0",
    "seedrandom": "^3.0.5",
    "unified": "^10.1.0",
    "vfile": "^5.1.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/lib/index.js",
      "!src/demo/index.js",
      "!src/utils/**",
      "!**/tests/**"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
